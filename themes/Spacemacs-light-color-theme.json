  "tokenColors": [
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#2aa1ae"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2d9574"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4e3163"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#4e3163"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#4e3163"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#715ab1",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#3a81c3"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#225588"
      }
    },
    {
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3a81c3"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#3a81c3"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#715ab1"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#3a81c3"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Library constants",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#4e3163"
      }
    },
    {
      "name": "Library classes/types",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#e0211d"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
}
